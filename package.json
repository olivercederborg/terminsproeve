{
	"private": true,
	"name": "remix-app-template",
	"description": "",
	"license": "",
	"scripts": {
		"postinstall": "remix setup node",
		"build": "npm run build:css && remix build",
		"build:css": "tailwindcss -m -i ./styles/tailwind.css -o app/styles/tailwind.css",
		"dev": "concurrently \"npm run dev:css\" \"remix dev\"",
		"dev:css": "tailwindcss -w -i ./styles/tailwind.css -o app/styles/tailwind.css"
	},
	"dependencies": {
		"@remix-run/react": "^1.1.3",
		"@remix-run/serve": "^1.1.3",
		"@remix-run/vercel": "^1.1.3",
		"@remix-validated-form/with-zod": "^2.0.1",
		"clsx": "^1.1.1",
		"framer-motion": "^6.2.6",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.3.1",
		"remix": "^1.1.3",
		"remix-validated-form": "^4.1.2",
		"zod": "^3.11.6"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.1.3",
		"@types/react": "^17.0.24",
		"@types/react-dom": "^17.0.9",
		"autoprefixer": "^10.4.2",
		"concurrently": "^7.0.0",
		"postcss": "^8.4.6",
		"tailwindcss": "^3.0.22",
		"typescript": "^4.1.2"
	},
	"engines": {
		"node": ">=14"
	},
	"sideEffects": false
